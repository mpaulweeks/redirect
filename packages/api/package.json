{
  "name": "redirect-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "init-1": "claudia generate-serverless-express-proxy --express-module src/app && rm package-lock.json",
    "init-2": "claudia create --handler lambda.handler --deploy-proxy-api --region us-east-1",
    "start": "nodemon src/app.local.js"
  },
  "dependencies": {
    "aws-serverless-express": "^3.3.5",
    "express": "^4.16.3"
  },
  "devDependencies": {
    "claudia": "^5.1.1",
    "nodemon": "^1.18.3"
  }
}
